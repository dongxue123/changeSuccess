/**index.wxss**/
.container{
    width:100%;
    height:100%;

}
.header{
    width:100%;
    
}
.header image{
   width:100%;
    height:440rpx;
    display:inline-block;
}
.list{
    width:100%;
    display:flex;
    /*padding:0 10rpx;*/
    box-sizing:border-box;
}
.list image{
    flex:1;
    width:350rpx;
    height:350rpx;
   margin:10rpx;
}
.swiper-tab{  
    width: 100%;  
    /*border-bottom: 2rpx solid #777777;  */
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 33.33%;  
    color: #777777;  
}  
.on{ color: #da7c0c; 
    font-weight: bold; 
    border-bottom: 5rpx solid #da7c0c;}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: center;  
}  